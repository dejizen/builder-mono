{
  "name": "@dejizen/builder",
  "version": "1.0.0",
  "private": false,
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "build:tw": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@dejizen/eslint-config": "workspace:*",
    "@dejizen/tailwind-config": "workspace:*",
    "@dejizen/typescript-config": "workspace:*",
    "@types/react": "^19.0.8",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.3.6",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@kontent-ai/delivery-sdk": "^15.2.0"
  }
}
